{
  "name": "widjet-utils",
  "version": "0.6.0",
  "description": "JS utilities for widjet widgets",
  "main": "lib/index.js",
  "jsnext:main": "src/index.es6",
  "scripts": {
    "test": "nyc mocha --timeout 10000 --recursive test/**/*.es6 test/*.es6",
    "lcov_report": "nyc report --reporter=lcov",
    "coverage": "npm run lcov_report && codecov",
    "babel": "babel src --out-dir lib",
    "lint": "eslint src/*.es6 test/*.es6",
    "lint:fix": "eslint --fix src/*.es6 test/*.es6",
    "esdoc": "esdoc -c esdoc.json",
    "prepublish": "npm run babel",
    "start": "widjet-test-server test/**/*.es6"
  },
  "keywords": [
    "widgets",
    "widget",
    "web",
    "widjet",
    "utilities"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/abe33/widjet-utils.git"
  },
  "bugs": "http://github.com/abe33/widjet-utils/issues",
  "commits": "http://github.com/abe33/widjet-utils/commits",
  "author": "Cédric Néhémie <cedric.nehemie@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-register": "^6.26.0",
    "codecov": "^1.0.0",
    "esdoc": "^1.1.0",
    "eslint": "^3.11.1",
    "eslint-config": "^0.3.0",
    "eslint-config-fbjs": "^1.1.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-jasmine": "^2.2.0",
    "eslint-plugin-prefer-object-spread": "^1.1.0",
    "eslint-plugin-react": "^5.2.2",
    "expect.js": "^0.3.1",
    "fbjs": "^0.8.6",
    "mocha": "^6.0.2",
    "mocha-jsdom": "^2.0.0",
    "nyc": "^13.3.0",
    "sinon": "^1.17.4",
    "standard": "^12.0.1",
    "widjet-test-utils": "^1.8.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-preset-es2015": "^6.24.1",
    "jsdom": "^9.8.3",
    "widjet-disposables": "^0.1.7"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "transform-es2015-classes",
        {
          "loose": true
        }
      ],
      "transform-es3-member-expression-literals",
      "transform-es3-property-literals"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "include": [
      "src/*.es6"
    ],
    "extension": [
      ".es6"
    ],
    "require": [
      "babel-register"
    ]
  }
}
